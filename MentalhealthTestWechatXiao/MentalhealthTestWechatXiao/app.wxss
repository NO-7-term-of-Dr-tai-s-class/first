/**app.wxss**/
@import "wxParse/wxParse.wxss";
@import "template/views/views.wxss"; 

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}  
   
.toWrite {
  width:90rpx;
  height:90rpx;
  display: block;
  background-size:cover;
  position:fixed;
  right:74rpx;
  bottom:100rpx;
  border-radius: 50%;
  background-color: #cdcdcd;
  opacity: 0.7;
  z-index: 999;
  border: 2rpx white solid;
  box-shadow: 0rpx 0rpx 2rpx #f0f0f0;
}

