<wxs src="../../wxs/utils.wxs" module="myutil" />
<view class='content'>
  <view class='nav'>
    <view class='top'>
      <open-data type="userAvatarUrl"></open-data>
    </view>
    <view class='nickname'>
      <open-data type="userNickName"></open-data> 
    </view>
  </view>

    <view class='item'>
      <text class='left'>姓名：</text><text class='right'>{{list.uname}}</text>
    </view>

    <view class='item'>
      <text class='left'>电话：</text><text class='right'>{{list.utel}}</text>
    </view>
    
    <view class='item' bindtap='resetpwd'>
     <text class='left'> 密码：</text> <text class='right'>{{list.upwd}}</text>
    </view>

     <view class='inform-btn'>
        <button class="vote-btn" type="primary" bindtap='upUserInfoBtn' >修改</button>
      </view>
  
</view>






